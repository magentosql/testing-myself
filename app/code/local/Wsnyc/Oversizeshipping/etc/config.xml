<?xml version="1.0"?>
<config>
    <modules>
        <Wsnyc_Oversizeshipping>
            <version>0.1.1</version>
        </Wsnyc_Oversizeshipping>
    </modules>
    <global>
        <models>
            <oversizeshipping>
                <class>Wsnyc_Oversizeshipping_Model</class>
            </oversizeshipping>
            <sales>
                <rewrite>
                    <quote_address>Wsnyc_Oversizeshipping_Model_Rewrite_Sales_Quote_Address</quote_address>
                </rewrite>
            </sales>
        </models>
        <resources>
            <oversizeshipping_setup>
                <setup>
                    <module>Wsnyc_Oversizeshipping</module>
                    <class>Wsnyc_Oversizeshipping_Model_Resource_Eav_Mysql4_Setup</class>
                </setup>
            </oversizeshipping_setup>
        </resources>
    </global>
</config>