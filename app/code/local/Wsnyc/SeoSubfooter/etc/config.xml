<?xml version="1.0"?>
<config>
    <modules>
        <Wsnyc_SeoSubfooter>
            <version>0.1</version>
        </Wsnyc_SeoSubfooter>
    </modules>
    <global>
        <blocks>
            <seosubfooter>
                <class>Wsnyc_SeoSubfooter_Block</class>
            </seosubfooter>
        </blocks>
        <helpers>
            <seosubfooter>
                <class>Wsnyc_SeoSubfooter_Helper</class>
            </seosubfooter>
        </helpers>
        <models>
            <seosubfooter>
                <class>Wsnyc_SeoSubfooter_Model</class>
                <resourceModel>seosubfooter_resource</resourceModel>
            </seosubfooter>
            <seosubfooter_resource>
                <class>Wsnyc_SeoSubfooter_Model_Resource</class>
                <entities>
                    <blurb>
                        <table>blurb</table>
                    </blurb>
                </entities>
            </seosubfooter_resource>
        </models>
        <resources>
            <seosubfooter_setup>
                <setup>
                    <module>Wsnyc_SeoSubfooter</module>
                    <class>Wsnyc_SeoSubfooter_Model_Resource_Setup</class>
                </setup>
                <connection>
                    <use>core_setup</use>
                </connection>
            </seosubfooter_setup>
            <seosubfooter_write>
                <connection>
                    <use>core_write</use>
                </connection>
            </seosubfooter_write>
            <seosubfooter_read>
                <connection>
                    <use>core_read</use>
                </connection>
            </seosubfooter_read>
        </resources>
    </global>
    <frontend>
        <layout>
            <updates>
                <seosubfooter>
                    <file>wsnyc/seosubfooter.xml</file>
                </seosubfooter>
            </updates>
        </layout>
    </frontend>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Wsnyc_SeoSubfooter after="Mage_Adminhtml">Wsnyc_SeoSubfooter_Adminhtml</Wsnyc_SeoSubfooter>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <adminhtml>
        <layout>
            <updates>
                <seosubfooter>
                    <file>wsnyc/seosubfooter.xml</file>
                </seosubfooter>
            </updates>
        </layout>
        <translate>
            <modules>
                <seosubfooter>
                    <files>
                        <default>Wsnyc_SeoSubfooter.csv</default>
                    </files>
                </seosubfooter>
            </modules>
        </translate>
    </adminhtml>
</config>