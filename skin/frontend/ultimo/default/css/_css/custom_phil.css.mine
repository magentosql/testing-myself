-------------------------------------------------------------- */

#nav {
	border-bottom: none;
}

.nav-dropdown-wrapper {
	position: absolute;
	top: 50px;
	left: 0;
	
	width: 927px;
	padding: 7px 6px 7px 7px;
	
	background-color: #fff;
	
	-moz-box-shadow: 1px 1px 5px rgba(0,0,0, 0.2);
	-webkit-box-shadow: 1px 1px 5px rgba(0,0,0, 0.2);
	box-shadow: 1px 1px 5px rgba(0,0,0, 0.2);
}

#nav ul.level0 {
	position: absolute;
	top: 50px;
	left: 0;		
	padding: 0;
	margin: 0;
        width: 100%;
}

#nav ul.level0 .nested-container {
        border: 1px solid black;
        overflow: hidden;  
        width: auto;
        margin: 7px;
        padding: 20px 30px;
}

        #nav ul.level0 .nested-container h4 {
            color: #222;
            font-family: 'Sorts Mill Goudy Regular', serif;
            font-size: 14px;
            font-style: normal;
            font-weight: normal;
            text-transform: uppercase;
        }
        
        #nav ul.level0 .nested-container ul.childcategories li {            
            background-image: none;
            border-top: 1px solid #ababab;
            color: #888888;
            cursor: pointer;
            font-family: 'GillSansMTStd', serif;
            font-size: 14px;
            font-style: normal;
            font-weight: normal;
            line-height: 30px;     
            padding-left: 5px;
            transition: color 300ms ease-out, background-color 300ms ease-out, border-color 300ms ease-out;
            -moz-transition: color 300ms ease-out, background-color 300ms ease-out, border-color 300ms ease-out;
            -webkit-transition: color 300ms ease-out, background-color 300ms ease-out, border-color 300ms ease-out;
            -o-transition: color 300ms ease-out, background-color 300ms ease-out, border-color 300ms ease-out;
        }
        
        #nav ul.level0 .nested-container ul.childcategories li:hover {
            background-color: #E9E9E9;
            color: #222222;
            border-color: #E9E9E9;
        }
        
        #nav ul.level0 .nested-container ul.childcategories li:hover + li {
            border-color: #E9E9E9;
        }
        
        #nav ul.level0 .nested-container ul.childcategories li:first-child {
            border-top: 0;
        }
        
        #nav ul.level0 .nested-container .custom-block:first-child {
            border-bottom: 1px solid #ababab;
            padding-bottom: 20px;
            margin-top: 35px;
        }        
        
        #nav ul.level0 .nested-container .custom-block {
            margin-bottom: 2em;
            text-align: center;            
        }
        
        #nav ul.level0 .nested-container .custom-block h4 {         
            background: url('../images/customblock-shortline.png') no-repeat bottom center;            
            font-family: "Bodoni-Book", sans-serif;
            font-size: 26px;
            font-style: italic;  
            padding-bottom: 14px;
            text-transform: none;            
        }
        
        #nav ul.level0 .nested-container .custom-block p {
            font-family: "GillSansMTStd";
            font-style: normal;
        }
        
        #nav ul.level0 .nested-container .custom-block a {
            float: none;
        }
                
