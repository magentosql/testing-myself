    font-family: "GillSansMTStd-Bold", sans-serif;
    font-size: 14px;
    font-weight: normal;
    color: #999999;
}

.product-view .add-monogram-content {
    -webkit-transition: opacity 1s ease-in-out, height 1s;
    -moz-transition: opacity 1s ease-in-out, height 1s;
    -o-transition: opacity 1s ease-in-out, height 1s;
    -ms-transition: opacity 1s ease-in-out, height 1s;    
    transition: opacity 1s ease-in-out, height 1s;
    height: 0;
    opacity: 0;
    position: relative;
    overflow: hidden;
}

.product-view .add-monogram-content.opened {
    height: 370px;
    opacity: 1;
}

.product-view .add-monogram-content #monogram-logo {
    position: absolute;
    top: 3px;
    left: 0;
}

.product-view .add-monogram-content h3 {
    text-transform: uppercase;
    margin-bottom: 0;
}

.product-view .add-monogram-content h3, .product-view .add-monogram-content p {
    font-family: "GillSansMTStd-Bold", sans-serif;
    font-size: 14px;
    font-weight: normal;
    color: #131425;
    margin-left: 55px;
}

.product-view .add-monogram-content p {
    margin-bottom: 1em;
}

.product-view .add-monogram-content .add-monogram-list {
    overflow: hidden;
    width: 400px;
}

.product-view .add-monogram-content .add-monogram-list > li {
    float: left;
    margin-right: 5px;
    margin-bottom: 5px;
    border: 1px solid #d7d7d7;
}

.product-view .add-monogram-content .add-monogram-list > li img {
    cursor: pointer;
}

.product-view .add-monogram-content .add-monogram-list > li.selected,
.product-view .add-monogram-content .add-monogram-list > li:hover {
    border: 1px solid #222;
} 

.product-view .add-monogram-content .add-monogram-process  {
    counter-reset:li; /* Initiate a counter */
    margin-left:1.7em; /* Remove the default left margin */
    padding-left:0; /* Remove the default left padding */
}
.product-view .add-monogram-content .add-monogram-process  > li {
    position:relative; /* Create a positioning context */    
    list-style:none; /* Disable the normal item numbering */    
}
.product-view .add-monogram-content .add-monogram-process  > li:before {
    content:counter(li) "."; /* Use the counter as content */
    counter-increment:li; /* Increment the counter by 1 */
    /* Position and style the number */
    position:absolute;
    top:0;
    left:-1.7em;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    box-sizing:border-box;
    width:2em;
    /* Some space between the number and the content in browsers that support
       generated content but not positioning it (Camino 2 is one example) */
    margin-right:8px;        
    font-family:"GillSansMTStd-Bold", sans-serif;
    color: #999999;
    text-align:center;
    font-size: 14px;
}

.product-view .add-monogram-content .add-monogram-process > li {
    margin-bottom: 1.2em;
}

.product-view .add-monogram-content .add-monogram-process > li > label {
    font-family: "GillSansMTStd-Bold", sans-serif;
